---
import BaseHead from '@components/BaseHead.astro';

export interface Props {
  content: {
    title: string;
    description: string;
  };
}

const {
  content: {
    title,
    description,

  },
} = Astro.props;
---

<html lang="en">
  <head>
    <BaseHead title={title} description={description} />
  </head>
  <body>
    <nav>
      <a href="/">Home</a>
      <a href="/about">About</a>
    </nav>
    <main>
      <slot />
    </main>
  </body>
</html>
